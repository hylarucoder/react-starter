{
  "name": "react-starter",
  "version": "1.0.1",
  "description": "react-starter",
  "scripts": {
    "postinstall": "husky install",
    "build": "vite build",
    "dev": "vite",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "vite"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["prettier --ignore-path .eslintignore --write"],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": ["prettier --ignore-path .eslintignore --parser json --write"],
    "*.{css,scss}": ["prettier --ignore-path .eslintignore --write"],
    "*.{html,md,yml}": ["prettier --ignore-path .eslintignore --write"]
  },
  "author": {
    "name": "twocucao",
    "email": "twocucao@gmail.com"
  },
  "homepage": "https://github.com/twocucao/react-starter",
